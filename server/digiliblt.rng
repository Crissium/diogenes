      <value>inapplicable</value>
    </choice>
  </define>
  <define name="data.language">
    <data type="language"/>
  </define>
  <define name="data.outputMeasurement">
    <data type="token">
      <param name="pattern">[\-+]?\d+(\.\d+)?(%|cm|mm|in|pt|pc|px|em|ex|gd|rem|vw|vh|vm)</param>
    </data>
